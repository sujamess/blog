    "deploy": "yarn build && yarn export"
  },
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "dayjs": "^1.9.6",
    "gray-matter": "^4.0.2",
    "next": "10.0.1",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.3.0",
    "postcss-flexbugs-fixes": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remark-prism": "^1.3.1",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "typescript": "^4.0.5"
  }
